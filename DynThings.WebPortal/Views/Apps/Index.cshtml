@*@model IEnumerable<DynThings.Data.Models.App>*@
@{
    ViewBag.Title = "Apps";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<div class="container">
    <div class="page-header">
        <h1>Apps</h1>
    </div>
    <div class="row">
        <div class="col-md-6">
            <button type="button" class="btn btn-info" onclick="LoadPart_DialogAppAdd()" data-toggle="modal" data-target="#mdl">Add</button>
        </div>
        <div class="col-md-6">
            <div class="input-group">
                <input type="search" id="txtAppSearch" class="form-control" onkeydown="if (event.keyCode == 13) { LoadPart_AppListDiv(); return false; }" />
                <div class="input-group-btn">
                    <button type="button" class="btn btn-primary" onclick="LoadPart_AppListDiv()">Search</button>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div class="well well-lg">
        <div id="divAppsList">
        </div>
    </div>
</div>
@section scriptfiles{
    <script src="~/Assets/Scripts/DynScripts/Apps.js"></script>
}
@section scripts{
    <script>
        $(document).ready(function () {
            LoadPart_AppListDiv();
            AttachEventAppsListPager();
        });


    </script>
}