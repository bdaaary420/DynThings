@model IEnumerable<DynThings.Data.Models.Location>
@{
    ViewBag.Title = "Locations";
}
<!--Page Title-->
<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
<div id="page-title">
    <div class="row">
        <div class="col-lg-9">
            <h1 class="page-header text-overflow">Locations</h1>
            <h5></h5>
        </div>

        <div class="col-lg-3">
            <div class="pull-right">

            </div>
        </div>
    </div>
</div>
<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
<!--End page title-->

<div id="page-content">
    <div class="row">
        <div class="col-xs-3 col-sm-2 col-md-6">
            <button type="button" class="btn btn-purple" onclick="LoadPart_DialogLocationAdd()" data-toggle="modal" data-target="#mdl">Add</button>
        </div>
        <div class="col-md-6">
            <div class="input-group">
                <input type="search" id="txtLocationsSearch" class="form-control" onkeydown="if (event.keyCode == 13) { LoadPart_LocationListDiv(); return false; }" />
                <div class="input-group-btn">
                    <button type="button" class="btn btn-primary" onclick="LoadPart_LocationListDiv()">Search</button>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div id="divLocationsList">
    </div>
</div>


@section scripts{
<script src="~/Assets/Scripts/DynScripts/Locations.js"></script>
    <script>
        $(document).ready(function () {
            LoadPart_LocationListDiv();
            
        });
    </script>
}