@model IEnumerable<DynThings.Data.Models.EndPointType>
@{
    ViewBag.Title = "EndPoint Types";
}
<div class="container">
    <div class="page-header">
        <h1>EndPoint Types</h1>
    </div>
    <div class="row">
        <div class="col-md-6 col-sm-2 col-xs-3">
            <button type="button" class="btn btn-info" onclick="LoadPart_DialogEndPointTypeAdd()" data-toggle="modal" data-target="#mdl">Add</button>
        </div>
        <div class="col-md-6 col-sm-10 col-xs-9">
            <div class="input-group">
                <input type="search" id="txtEndPointTypeSearch" class="form-control" onkeydown="if (event.keyCode == 13) { LoadPart_EndPointTypeListDiv(); return false; }" />
                <div class="input-group-btn">
                    <button type="button" class="btn btn-primary" onclick="LoadPart_EndPointTypeListDiv()">Search</button>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div class="well well-lg">
        <div id="divEndPointTypesList">
        </div>
    </div>
</div>
@section scriptfiles{
    <script src="~/Assets/Scripts/DynScripts/EndPointTypes.js"></script>
}
@section scripts{
    <script>
        $(document).ready(function () {
            LoadPart_EndPointTypeListDiv();
            AttachEventEndPointTypesListPager();
        });

       
    </script>
}